{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*": ["src/layouts/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["vidstack/solid"]
  }
}
